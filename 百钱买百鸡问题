#include <iostream>

using namespace std;

int main(){
    int a,b,c;
    int result = 0;
    for(a=0;a<=25;a++){
        for(b=0;b<=33;b++){
            result = 100 - a*5 - b*3;
            if(result <= 0)
                break;
            c = result*3;
            if(a+b+c == 100){
                cout<< a << " " << b << " " << c << endl;
                break;
            }
        }
    }
    return 0;
}